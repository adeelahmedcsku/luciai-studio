{
    "identifier": "default",
    "description": "Default capabilities for the app",
    "local": true,
    "windows": [
        "main"
    ],
    "permissions": [
        "core:default",
        "shell:default",
        "shell:allow-open",
        "dialog:default",
        "fs:default",
        "fs:allow-read-dir",
        "fs:allow-read-file",
        "fs:allow-write-file",
        "fs:allow-exists",
        "fs:allow-mkdir",
        "core:window:allow-start-dragging"
    ]
}